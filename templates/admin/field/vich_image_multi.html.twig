{# admin/field/vich_image.html.twig #}
{% for image in field.value %}
    {% set html_id = 'ea-lightbox-' ~ field.uniqueId ~ '-' ~ loop.index %}
    <a href="#" class="ea-lightbox-thumbnail" data-ea-lightbox-content-selector="#{{ html_id }}">
        <img src="{{ vich_uploader_asset(image) }}"
             class="img-fluid" alt="">
    </a>

    <div id="{{ html_id }}" class="ea-lightbox">
        <img src="{{ vich_uploader_asset(image) }}" alt="">
    </div>
    {#    {{ dump(entity.instance, field.property, entity.fqcn, entity.name) }} #}

{% endfor %}
