{
  "type": "project",
  "license": "proprietary",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": ">=8.3",
    "ext-zip": "*",
    "api-platform/core": "4.*",
    "cboden/ratchet": "^0.4.4",
    "doctrine/doctrine-bundle": "2.*",
    "doctrine/doctrine-migrations-bundle": "3.*",
    "doctrine/orm": "3.*",
    "easycorp/easyadmin-bundle": "4.*",
    "intervention/image": "^3.11",
    "nelmio/cors-bundle": "2.*",
    "phpdocumentor/reflection-docblock": "5.*",
    "phpstan/phpdoc-parser": "1.*",
    "symfony/apache-pack": "1.*",
    "symfony/asset": "7.*",
    "symfony/console": "7.*",
    "symfony/doctrine-messenger": "7.*",
    "symfony/dom-crawler": "*",
    "symfony/dotenv": "7.*",
    "symfony/expression-language": "7.*",
    "symfony/flex": "2.*",
    "symfony/form": "7.*",
    "symfony/framework-bundle": "7.*",
    "symfony/http-client": "*",
    "symfony/intl": "7.*",
    "symfony/mailer": "7.*",
    "symfony/mime": "7.*",
    "symfony/monolog-bundle": "3.*",
    "symfony/notifier": "7.*",
    "symfony/process": "7.*",
    "symfony/property-access": "7.*",
    "symfony/property-info": "7.*",
    "symfony/runtime": "7.*",
    "symfony/security-bundle": "7.*",
    "symfony/security-core": "7.*",
    "symfony/serializer": "7.*",
    "symfony/string": "7.*",
    "symfony/translation": "*",
    "symfony/twig-bundle": "7.*",
    "symfony/validator": "7.*",
    "symfony/web-link": "7.*",
    "symfony/yaml": "7.*",
    "twig/extra-bundle": "3.*",
    "twig/twig": "3.*",
    "vich/uploader-bundle": "^2.5"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true,
      "symfony/flex": true,
      "symfony/runtime": true
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php74": "*",
    "symfony/polyfill-php80": "*",
    "symfony/polyfill-php81": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "*"
    }
  },
  "require-dev": {
    "doctrine/doctrine-fixtures-bundle": "3.*",
    "phpunit/phpunit": "11.*",
    "symfony/browser-kit": "7.*",
    "symfony/css-selector": "7.*",
    "symfony/debug-bundle": "7.*",
    "symfony/maker-bundle": "1.*",
    "symfony/phpunit-bridge": "7.*",
    "symfony/stopwatch": "7.*",
    "symfony/web-profiler-bundle": "7.*"
  }
}
